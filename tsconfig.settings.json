{

    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es5",
        "declaration": true,
        "stripInternal": true,
    
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
    
        "moduleResolution": "node",
        "inlineSourceMap": true,
        "inlineSources": true,
    
        "experimentalDecorators": true
    }
  }
  